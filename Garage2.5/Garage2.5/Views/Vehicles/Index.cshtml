@model IEnumerable<Garage2._5.Models.Vehicle>

@{
    ViewBag.Title = "Vehicles";
}

<h2>Detail View of Vehicles</h2>
@using (Html.BeginForm())
{
    <p>
        @Html.TextBox("SearchString")
        <input type="submit" value="Sök" />
    </p>
}
<p>
    @Html.ActionLink("Park-Inn", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.ActionLink("Owner", "Index", new { sortOrder = ViewBag.MemberNameSortParm, searchString = "" })
           @* @Html.DisplayNameFor(model => model.Members.Name)*@
        </th>
        <th>
            @Html.ActionLink("Type", "Index", new { sortOrder = ViewBag.TypSortParm, searchString = "" })
            @*@Html.DisplayNameFor(model => model.VehicleTypes.Type)*@
        </th>
        <th>
            @Html.ActionLink("Registering Number", "Index", new { sortOrder = ViewBag.RegNrSortParm, searchString = "" })
            @*@Html.DisplayNameFor(model => model.RegistrationNumber)*@
        </th>
        
        
        <th>
            @Html.ActionLink("Parking Time", "Index", new { sortOrder = ViewBag.TidSortParm, searchString = "" })
            @*@Html.DisplayNameFor(model => model.CheckIn)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Members.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleTypes.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationNumber)
        </td>
        
        
        <td>
            @Html.DisplayFor(modelItem => item.CheckIn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Check-Out", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
