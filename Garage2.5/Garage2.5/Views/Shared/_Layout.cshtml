<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Garage 2.0</title>
    @Styles.Render("https://fonts.googleapis.com/css?family=Monoton")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Greeen <br />Garage</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="active">
                    <li>@Html.ActionLink("Members", "Index", "Members")<span class="sr-only">(current)</span></li>
                    <li>@Html.ActionLink("Vehicles", "Index", "Vehicles")</li>
                    <li>@Html.ActionLink("VehicleTypes", "Index", "VehicleTypes")</li>
                    <li>@Html.ActionLink("Statistics", "Stats", "Vehicles")</li>

                </ul>


            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>Copy Wirte &copy;2017 Protected By Thomas Unger -- Dildar Ahmad Khan </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
